<% if(typeof errors!= 'undefined') { %>
  <%    errors.forEach(function(error){ %>
          <p class="text-danger"> <%= error.msg %></p>
  <%    })        %>
  <% } %>
  
  <% if(success_msg!= '') { %>
          <p class="text-danger"><%=success_msg %> </p>
        <% } %>
  
  <% if(error_msg != '') { %>
        <p class="text-danger"><%= error_msg %></p> 
        <% } %> 
  <% if(error != '') { %>
         <p class="text-danger"><%= error %> </p>
        <% } %> 